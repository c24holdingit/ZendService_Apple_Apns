{
    "name": "zendframework/zendservice-apple-apns",
    "description": "OOP Zend Framework 2 wrapper for Apple Push Notification Service",
    "type": "library",
    "keywords": [
        "zf2",
        "apns",
        "push",
        "notification",
        "apple"
    ],
    "homepage": "http://packages.zendframework.com/",
    "license": "BSD-3-Clause",
    "autoload": {
        "psr-4": {
            "ZendService\\Apple\\": "library/ZendService/Apple/",
            "ZendServiceTest\\Apple\\Apns\\": "tests/ZendService/Apple/Apns"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "http://packages.zendframework.com/"
        }
    ],
    "require": {
        "php": ">=5.5.24",
        "lib-curl": ">=7.47",
        "zendframework/zend-json": ">=2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0"
    },
    "config": {
        "secure-http": false
    }
}
